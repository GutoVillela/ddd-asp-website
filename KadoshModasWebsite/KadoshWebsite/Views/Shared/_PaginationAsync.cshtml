<!-- Pagination -->
<nav aria-label="navigation">
    <ul class="pagination justify-content-end">
        <li class="page-item @(Model.CurrentPage == 1 ? "disabled" : string.Empty)">
            <a class="page-link" onclick="@ViewData["PaginationJSMethod"] (@(Model.CurrentPage - 1))">Anterior</a>
        </li>
        @for (int i = Model.StartPage; i <= Model.EndPage; i++)
        {

            <li class="page-item @(Model.CurrentPage == i ? "active disabled''" : string.Empty)">
                <a class="page-link" onclick="@ViewData["PaginationJSMethod"] (@i)">@i</a>
            </li>
        }
        <li class="page-item @(Model.CurrentPage == Model.TotalPages ? "disabled" : string.Empty)">
            <a class="page-link" onclick="@ViewData["PaginationJSMethod"] (@(Model.CurrentPage + 1))">Próximo</a>
        </li>
    </ul>
</nav>
<!-- END Pagination -->