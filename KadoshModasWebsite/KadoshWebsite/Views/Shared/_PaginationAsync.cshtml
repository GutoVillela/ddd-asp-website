<!-- Pagination -->
<nav aria-label="navigation">
    <ul class="pagination justify-content-end">
        <li class="page-item @(Model.CurrentPage == 1 ? "disabled" : string.Empty)">
            <button class="page-link" onclick="@ViewData["PaginationJSMethod"] (@(Model.CurrentPage - 1))">Anterior</button>
        </li>
        @for (int i = Model.StartPage; i <= Model.EndPage; i++)
        {

            <li class="page-item @(Model.CurrentPage == i ? "active" : string.Empty)">
                <button class="page-link" onclick="@ViewData["PaginationJSMethod"] (@i)">@i</button>
            </li>
        }
        <li class="page-item @(Model.CurrentPage == Model.TotalPages ? "disabled" : string.Empty)">
            <button class="page-link" onclick="@ViewData["PaginationJSMethod"] (@(Model.CurrentPage + 1))">Próximo</button>
        </li>
    </ul>
</nav>
<!-- END Pagination -->